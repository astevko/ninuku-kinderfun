<?xml version="1.0" encoding="UTF-8"?>
<!-- DetailsViewImpl.ui.xml -->

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget">
 <g:HTMLPanel>
	<mgwt:LayoutPanel ui:field="main">
		<mgwt:HeaderPanel>
		    <mgwt:left>
				<mgwt:HeaderButton ui:field="backButton" text="back" backButton="true"></mgwt:HeaderButton>
			</mgwt:left>
		    <mgwt:center>
				<g:HTML ui:field="name"></g:HTML>
			</mgwt:center>
		    <mgwt:right>
				<g:HTML ui:field="id"></g:HTML>
			</mgwt:right>
		</mgwt:HeaderPanel>

		<mgwt:ScrollPanel scrollingEnabledX="false">
			<g:FlowPanel>
				<mgwt:RoundPanel>
					<g:HTML ui:field="desc"></g:HTML>
				</mgwt:RoundPanel>
				<!-- mgwt:Button ui:field="editButton" text="edit"></mgwt:Button -->
			</g:FlowPanel>
		</mgwt:ScrollPanel>
	</mgwt:LayoutPanel>
</g:HTMLPanel>
</ui:UiBinder>